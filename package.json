{
  "name": "rps-bot",
  "description": "A bot that lets users play rock, paper, scissors against each other.",
  "version": "0.2.1",
  "author": "tarolling",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config index.js",
    "dev": "node -r dotenv/config index.js dev",
    "reload": "node -r dotenv/config ./scripts/deploy-commands.js",
    "reload-dev": "node -r dotenv/config ./scripts/deploy-commands.js dev",
    "stats": "node -r dotenv/config ./scripts/post-stats.js",
    "lint": "eslint **/*.js",
    "fix": "eslint **/*.js --fix",
    "precommit": "npm run lint && npm run test"
  },
  "dependencies": {
    "@discordjs/rest": "^2.4.0",
    "async-mutex": "^0.5.0",
    "bfd-api-redux": "^1.2.3",
    "browser-sync": "^3.0.2",
    "discord.js": "^14.16.3",
    "discord-api-types": "^0.37.108",
    "discordbotlist": "^1.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongodb": "^6.10.0",
    "topgg-autoposter": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^9.13.0"
  },
  "nodeVersion": "21.6.2"
}
